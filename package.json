{
    "name": "darkmannn-blog",
    "version": "1.0.0",
    "description": "Darkmannn's personal blog",
    "scripts": {
        "start": "ng serve --port 8080",
        "watch:1": "ng build --watch --configuration development",
        "watch:2": "scully --scanRoutes --watch",
        "build": "ng build --prod && scully --prod --scanRoutes",
        "insert": "ng generate @scullyio/init:post --target blog-posts --meta-data-file scripts/default-frontmatter.yml --name $1"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.1",
        "@angular/common": "~13.2.1",
        "@angular/compiler": "~13.2.1",
        "@angular/core": "~13.2.1",
        "@angular/forms": "~13.2.1",
        "@angular/platform-browser": "~13.2.1",
        "@angular/platform-browser-dynamic": "~13.2.1",
        "@angular/router": "~13.2.1",
        "@scullyio/init": "^2.1.11",
        "@scullyio/ng-lib": "^2.1.11",
        "@scullyio/scully": "^2.1.11",
        "@scullyio/scully-plugin-puppeteer": "^2.1.11",
        "angular-svg-icon": "^12.0.0",
        "normalize.css": "^8.0.1",
        "prismjs": "^1.25.0",
        "rxjs": "^7.5.2",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.2",
        "@angular/cli": "~13.2.2",
        "@angular/compiler-cli": "~13.2.1",
        "@types/node": "^12.11.1",
        "@types/prismjs": "^1.16.6",
        "prettier": "~2.3.2",
        "typescript": "~4.5.5"
    },
    "keywords": [
        "blog"
    ],
    "author": "DarkMannn"
}
